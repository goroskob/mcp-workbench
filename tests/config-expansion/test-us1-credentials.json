{
  "toolboxes": {
    "test-credentials": {
      "description": "Test environment variable expansion for secure credentials",
      "mcpServers": {
        "memory-server": {
          "command": "npx",
          "args": ["-y", "@modelcontextprotocol/server-memory", "${DATABASE_PASSWORD}"],
          "env": {
            "API_KEY": "${API_KEY}",
            "DATABASE_PASSWORD": "${DATABASE_PASSWORD}",
            "AUTH_TOKEN": "${AUTH_TOKEN}"
          }
        }
      }
    }
  }
}
