{
  "toolboxes": {
    "test-multienv": {
      "description": "Test environment variable expansion for multi-environment configurations",
      "mcpServers": {
        "api-server": {
          "command": "npx",
          "args": [
            "-y",
            "@modelcontextprotocol/server-fetch"
          ],
          "env": {
            "API_ENDPOINT": "${API_ENDPOINT}",
            "API_VERSION": "${API_VERSION}",
            "AUTH_TOKEN": "${AUTH_TOKEN}",
            "LOG_LEVEL": "${LOG_LEVEL}",
            "ENVIRONMENT": "${ENVIRONMENT}"
          }
        }
      }
    }
  }
}
